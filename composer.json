{
  "name": "cesi/core",
  "description": "Funciones para el desarrollo de un erp",
  "keywords": [
    "erp",
    "negocio"
  ],
  "type": "library",
  "license": "proprietary",
  "homepage": "https://github.com/carless/V6",
  "authors": [
    {
      "name": "Carles Serrats",
      "email": "carless@cesigrup.com",
      "homepage": "http://www.serrats.cat",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "^7.2",
    "laravel/framework": "^6.0",
    "spatie/laravel-permission": "^3.0",
    "doctrine/dbal": "^2.5",
    "laravelcollective/html": "^6.0",
    "prologue/alerts": "^0.4.1",
    "nesbot/carbon": "^2.14.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Cesi\\Core\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Cesi\\Core\\CesiCoreServiceProvider"
      ]
    }
  }
  "minimum-stability": "dev",
  "prefer-stable": true
}